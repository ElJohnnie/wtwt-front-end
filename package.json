{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"**/*.{ts,tsx}\" --max-warnings=0",
    "lint:fix": "eslint \"**/*.{ts,tsx}\" --fix --max-warnings=0",
    "format": "prettier --write .",
    "reinstall": "rm -rf node_modules package-lock.json && npm i",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:unit": "jest --coverage",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "check-coverage": "jest --coverage --no-cache"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.3",
    "axios": "^1.7.2",
    "husky": "^8.0.3",
    "next": "14.2.3",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.4.0",
    "@storybook/addon-backgrounds": "^8.1.3",
    "@storybook/addon-essentials": "^8.1.2",
    "@storybook/addon-interactions": "^8.1.2",
    "@storybook/addon-links": "^8.1.2",
    "@storybook/addon-onboarding": "^8.1.2",
    "@storybook/blocks": "^8.1.2",
    "@storybook/nextjs": "^8.1.2",
    "@storybook/react": "^8.1.2",
    "@storybook/test": "^8.1.2",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "istanbul": "^0.4.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "json-server": "^1.0.0-beta.0",
    "postcss": "^8",
    "prettier": "^2.8.2",
    "storybook": "^8.1.2",
    "tailwindcss": "^3.4.1",
    "typescript": "4.7.4"
  }
